<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="0.0" minWidth="0.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mil.navy.monitoring.view.BrowserLayoutController">
   <top>
      <Label fx:id="title" text="WebSite Name" BorderPane.alignment="CENTER">
         <font>
            <Font name="Malgun Gothic Bold" size="24.0" />
         </font>
         <BorderPane.margin>
            <Insets bottom="3.0" />
         </BorderPane.margin>
      </Label>
   </top>
   <center>
      <WebView fx:id="browser" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin></WebView>
   </center>
</BorderPane>
